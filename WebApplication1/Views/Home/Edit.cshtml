
@model HRM.DAL.Models.User
@{
    ViewBag.Title = "Edit";
}




<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>
    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)
        <fieldset>
            <div class="form-horizontal">
                <h4>Edit User</h4>

                <div class="form-group">
                    @Html.LabelFor(i => i.FullName)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.FullName)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.Email)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.Email)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.LevelId)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.LevelId)
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.Password)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.Password)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.StartDate)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.StartDate)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.StatusId)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.StatusId)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.UserRole)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.UserRole)

                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(i => i.UserTeam)
                    <div class="col-md-10">
                        @Html.EditorFor(i => i.UserTeam)

                    </div>
                </div>

              
                </div>
                <p>
                    <input type="submit" value="Save" />
                </p>
</fieldset>
    }
    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
</body>
</html>
